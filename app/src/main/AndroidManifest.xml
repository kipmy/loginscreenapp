<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kendrick.loginscreen">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/king"
        android:supportsRtl="true"
        android:theme="@style/Theme.Loginscreen"
        tools:targetApi="31">
        <activity
            android:name="com.kendrick.loginscreen.ViewproductsActivity"
            android:exported="false" />
        <activity
            android:name="com.kendrick.loginscreen.AddproductActivity"
            android:exported="false" />
        <activity
            android:name="com.kendrick.loginscreen.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.kendrick.loginscreen.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>